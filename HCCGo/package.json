{
  "name": "HCCGo",
  "version": "0.4.0-beta.1",
  "main": "./main.js",
  "description": "Interface for HCC Resouces",
  "scripts": {
    "start": "node node_modules/electron/cli.js .",
    "packageWin": "node ./node_modules/electron-packager/cli.js ./ HCCGo --platform=win32 --arch=x64 --out ../packageWin/ --electron-version 1.4.3 --overwrite --icon ./icons/HCCGo",
    "packageOsx": "node ./node_modules/electron-packager/cli.js ./ HCCGo --platform=darwin --arch=x64 --out ../packageOsx/ --electron-version 1.4.3 --overwrite --icon ./icons/HCCGo --osx-sign",
    "packageNix": "node ./node_modules/electron-packager/cli.js ./ HCCGo --platform=linux --arch=x64 --out ../packageNix/ --electron-version 1.4.3 --overwrite --icon ./icons/HCCGo",
    "postinstall": "node postinstall.js",
    "test": "./node_modules/.bin/karma start --single-run --browsers Electron"
  },
  "keywords": [
    "HCC"
  ],
  "author": "Derek Weitzel",
  "homepage": "https://github.com/unlhcc/HCCGo",
  "dependencies": {
    "async": ">= 1.5.2",
    "c3": ">= 0.4.11",
    "csv": "^1.1.0",
    "csv-parse": ">= 1.1.1",
    "d3": "^3.5.17",
    "diskusage": ">= 0.1.5",
    "electron-debug": "^1.0.1",
    "fs": ">= 0.0.2",
    "nedb": "^1.8.0",
    "node-notifier": "^4.6.0",
    "path": ">= 0.12.7",
    "ssh2": ">= 0.3.3",
    "uuid": "^3.0.1",
    "escape-html": ">=1.0.3"
  },
  "devDependencies": {
    "asar": "^0.12.3",
    "devtron": "^1.2.1",
    "electron": "^1.4.15",
    "electron-packager": ">= 5.0.1",
    "electron-rebuild": ">= 1.1.5",
    "grunt-marked": "^0.1.3",
    "jasmine": "^2.5.3",
    "karma": "^1.4.1",
    "karma-electron": "^5.1.1",
    "karma-jasmine": "^1.1.0",
    "nslog": "^3.0.0"
  }
}
